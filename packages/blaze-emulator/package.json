{
  "name": "@blazecardano/emulator",
  "version": "0.0.1",
  "description": "Blaze cardano emulator library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external rxjs",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external rxjs",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "vitest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@blazecardano/tsconfig": "workspace:*",
    "eslint": "^9.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@blazecardano/core": "workspace:*",
    "@blazecardano/query": "workspace:*",
    "@blazecardano/tx": "workspace:*",
    "@blazecardano/wallet": "workspace:*"
  }
}
